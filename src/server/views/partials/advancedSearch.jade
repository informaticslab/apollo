.row
	.div(ng-class='getContentWidth()')
		.content_block
			.subnav
				ul.nav.nav-pills
					li
						a(href='#/quickGuide/') Quick Guide
					li
						a(href='#/browse/') Browse
					li
						a(href='#/search/{{nodeId}}') Search
					li.active
						a(href='#/advancedSearch/') Advanced Search
					li
						a(href='' class='inactive') Favorites
					li
						a(href='' class='inactive') In the Lab
			// end subnav
			//- .backlink
			//- 	span.glyphicon.glyphicon-circle-arrow-left
			//- 	| &nbsp;Back to search results
			// end backlink class
			//- .entity_name
			//- 	| &nbsp;{{node.name}}
			.nav_split
				.btn-group.btn-group-md.btn-group-justified
					.btn-group
						a.btn.btn-default(href="#/node/{{nodeId}}", id='btnDetails') Details
					.btn-group
						a.btn.btn-custom(href='#/linkage/{{nodeId}}', id='btnLinkageViewer') Linkage Viewer
				// end button group
			// end nav_split
			.detail_header
				.share_button_linkage
					//These lines, 173 - 187, were added 
					//Single button 
					.btn-group
						button.btn.btn-default.dropdown-toggle(type='button', data-toggle='dropdown')
							span.glyphicon.glyphicon-share
							| &nbsp;Share&nbsp;&nbsp;
							span.caret
						ul.dropdown-menu(role='menu')
							li
								a(href='#') (Coming soon)
							li
								a(href='#') Email
							li
								a(href='#') Facebook
							li
								a(href='#') Twitter
						//end added lines 
				button.btn.btn-default.pull-left.link_button1(type='button')
					span.glyphicon.glyphicon-repeat.icon-flipped
					| Reset
				img.pull-left.small_divider(src='img/small_divider.png')
				button.btn.btn-default.pull-left.link_button5(type='button')
					i.fa.fa-lock
					| Lock nodes
				button.btn.btn-default.pull-left.link_button4(type='button')
					span.glyphicon.glyphicon-ban-circle
					| Hide lines					
				img.pull-left.small_divider(src='img/small_divider.png')
				.cluster
					.cluster_text
					| Organize by:&nbsp;&nbsp;
					.btn-group
						button.btn.btn-default.link_button6(type='button')
							i.fa.fa-gears
							| Activity type
						button.btn.btn-default.link_button7.disabled(type='button')
							span.glyphicon.glyphicon-link
							| Relationship type
							
				input(type="text" ng-model="nodeA"  placeholder="Activity A" typeahead-on-select="itemSelectedA($item, $model, $label)" typeahead-min-length='3' typeahead="node.name for node in getNodes($viewValue)" class="form-control")			
				input(type="text" ng-model="nodeB"  placeholder="Activity B" typeahead-on-select="itemSelectedB($item, $model, $label)" typeahead-min-length='3' typeahead="node.name for node in getNodes($viewValue)" class="form-control")							
				input(type="hidden" id="nodeAId" ng-value="nodeAId")
				input(type="hidden" id="nodeBId" ng-value="nodeBId")
			.detail_header
				.share_button_linkage
					| Number of hops
					input(type='radio', id='hop2' name='hop', value='2') 
					| 2
					input(type='radio', id='hop3' name='hop', value='3') 
					| 3
					input(type='radio', id='hop4' name='hop', value='4') 
					| 4
					input.pull-left.form-control(type='text',ng-model='leftnode',id='leftnode')
					input.pull-left.form-control(type='text',ng-model='rightnode',id='rightnode')
					button.pull-left.btn.btn-default.pull-left.link_buttone(type='button')
						span.glyphicon.glyphicon-ban-circle
						| Find
					
			// end detail header
			.top_block2
				.block_linkage
					.vertical_spacing1

						script(type="text/javascript", src="/apollo/app/d3/d3-advancedsearch.js")

				// end block_linkage
				.key
					table.key_details
						tr
							td.key_td
								.blue_subhead Key:
							td.node_image_td
								img(src='img/node.png')
							td.node_text_td
								strong Node:
								|   Represents an entity, such as Program, System, Dataset,
								|  Data Standard, Health Survey, Tool, Registry, Organization, Tag, or Collaborative.
								| Entity types are differentiated by node color.
							td.white_line
								img(src='img/white_divider.png') Â 
							td.line_image_td
								img(src='img/line.png')
							td.line_text_td
								|  Represents the relationship between any two entities,
								|  such as oversees, manages, uses, shares data with, etc. The arrow indicates
								|  the direction of the relationship (e.g., the source to the recipient). 
				// end class key
				.legend
					table.legend_details
						tr
							td.legend_td
								.blue_subhead Legend:
							td.node_legend_image_td
								img.move_legend(src='img/node_ss.png')
								| Surveillance System
							td.node_legend_image_td
								img.move_legend(src='img/node_program.png')
								| Program
							td.node_legend_image_td
								img.move_legend(src='img/node_registry.png')
								| Registry
							td.node_legend_image_td
								img.move_legend(src='img/node_hs.png')
								| Health Survey
						tr
							td.legend_td
								.blue_subhead
							td.node_legend_image_td
								img.move_legend(src='img/node_tool.png')
								| Tool
							td.node_legend_image_td
								img.move_legend(src='img/node_dataset.png')
								| Dataset
							td.node_legend_image_td
								img.move_legend(src='img/node_ds.png')
								| Data Standard		
							td.node_legend_image_td
								img.move_legend(src='img/node_col.png')
								| Collaborative			
						tr
							td.legend_td
								.blue_subhead
							td.node_legend_image_td
								img.move_legend(src='img/node_org.png')
								| Organization
							td.node_legend_image_td
								img.move_legend(src='img/node_tag.png')
								| Tag
				// end class legend				
				.instructions
					.blue_subhead Instructions:
					.left_bullets
						ul
							li.instruct
								| To 
								strong  view a node's relationships
								| , click on a
								| node name. This will bring it to the center of the graph. 
							li.instruct
								| To 
								strong  view more information 
								| about the center node, click the
								em  
									strong Details
								|  tab. 
							li.instruct
								| To
								strong  lock or reposition
								|  an individual node, click the node.
								| A black border will display around the node, and it will be locked in position. You may then drag the node to the desired position.
							li.instruct
								| To 
								strong lock all nodes
								| , click the 
								em 
									strong Lock nodes
								|  button. To release the nodes, simply click the button again, or click the 
								em
									strong Reset
								|  button.
					// end class left_bullets
					.right_bullets
						ul
							li.instruct
								| To
								strong  hide the lines
								|  in the graph, click the
								em
									strong  Hide lines
								|  button. To make them reappear, simply click the button again.	
							li.instruct
								| To 
								strong organize 
								|  the nodes by 
								strong  activity type
								| , click the 
								em
									strong  Activity type
								|  button. To disperse the nodes, simply click the button again.
							// li.instruct
							//	| To 
							//	strong cluster 
							//	|  the nodes by 
							//	strong  relationship type
							//	| , click the
							//	em
							//		strong  Relationship type
							//	|  button. To disperse the nodes, simply click the button again.
					// end class right_bullets
				// end class instructions
			// end top_block2
		// end content block
