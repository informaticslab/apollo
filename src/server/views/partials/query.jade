// new row
.row
	.div(ng-class='getContentWidth()')
		.content_block_faq
			span.visible-md.visible-lg
				.subnav
					ul.nav.nav-pills
						li
							a(href='#/quickGuide/') Quick Guide
						li
							a(href='#/browse/') Browse
						li
							a(href='#/search/') Search
						li
							a(href='#/advancedSearch/') Relationship Explorer
						li
							a(href='' class='inactive') Favorites
						li.active
							a(href='#/inTheLab/') In the Lab
				// end subnav
			span.visible-sm
				.subnav_sm
					ul.nav.nav-pills
						li
							a(href='#/quickGuide/') Quick Guide
						li
							a(href='#/browse/') Browse
						li
							a(href='#/search/') Search
						li
							a(href='#/advancedSearch/') Relationship Explorer
						li
							a(href='' class='inactive') Favorites
						li.active
							a(href='#/inTheLab/') In the Lab
				// end subnav
			//- a.btn.btn-custom(onclick='goBack()') Back
			//- .nav_split
			//- 	.btn-group.btn-group-md.btn-group-justified
			//- 		.btn-group
			//- 			a.btn.btn-custom(href="#/dashboard", id='btnDashboard') Summary
			//- 		.btn-group
			//- 			a.btn.btn-default(href='#/dashboarddetail', id='btnDashboardDetail') Detail
			//- 	// end button group
			//- // end nav_split
			.top_block_faq
				h4.orange_main_faq(ng-click="print()")
					span.glyphicon.glyphicon-comment.icon-flipped.faq_icon
					| Ad Hoc Query Tool
				.vertical_spacing_browse
				.vertical_spacing_lesser
				.row
					.col-lg-6.col-md-6.col-sm-6.gray_text(ng-hide="q_actname")
						.orange_header_larger
							| 1: Name of the activity to search
						.query-nobordered				
							.txtb(ng-repeat="txtcount in txtboxcount")
								.row
									.col-lg-10.col-md-10.col-sm-10
										input(id="{{'stxt_'+txtcount.tcount}}" type="text" ng-model="txtboxval[txtcount.tcount]"  placeholder="Search activity" typeahead-on-select="itemSelected($item, $model, $label,\"{{'stxt_'+txtcount.tcount}}\")" typeahead-min-length='3' typeahead="node.name for node in getNodes($viewValue)" class="form-control")
									.col-lg-2.col-md-2.col-sm-2
										label.btn.pull-right(ng-if="txtcount.tcount>0" ng-click="removetxtbox(txtcount.tcount)")
											i.fa.fa-times
								.vertical_spacing_lesser
							label.btn.btn-default( ng-click="addtxtbox()")
								i.fa.fa-plus
						//.vertical_spacing_less
						//label.btn.btn-default(ng-click="values()" ng-disabled="searchbtndisabled")
							i.fa.fa-chevron-circle-right
							| Search
					.col-lg-6.col-md-6.col-sm-6.gray_text(ng-hide="q_acttypes")
						.orange_header_larger
							| 2: Related activity types
						.query-bordered
							.checkbox(ng-repeat="activitytype in activitytypes")
								input(type="checkbox" ng-model="chkactvs[activitytype.nodetypes]" ng-change="checkrelbtnstatus()")
								| {{activitytype.nodetypes  | unCamelCase}}
						.vertical_spacing_less
						label.btn.btn-default(ng-click="getrelatedactivitytypes()" ng-disabled="nextrelbtndisabled")
							i.fa.fa-chevron-circle-right
							| Search
					//.col-lg-4.col-md-4.col-sm-4.gray_text(ng-hide="q_rel")
						.orange_header_larger
							| 2: Select relationships with these activities
						.query-bordered
							.checkbox(ng-repeat="rel in reltypes")
								input(type="checkbox" ng-model="chkrels[rel.relname]" ng-change="checkactbtnstatus()")
								| {{rel.relname  | TitleCase | unCamelCase}}
						.vertical_spacing_less
						label.btn.btn-default( ng-click="nextactname()" ng-disabled="nextactbtndisabled")
							i.fa.fa-chevron-circle-right
							| Next
					
					//- .col-lg-3.col-md-3.col-sm-3.gray_text(ng-hide="q_search")
					//- 	.orange_header_larger
					//- 		| 4: Click search
					//- 	//.vertical_spacing_more
