										| Path Nodes
									//th
										//a(href="" ng-click="reverse=!reverse;orderByField='name'")
										| Path Links
							tbody
								tr( ng-repeat="row in adhocresultspag")
									td(data-title="'Name'")
										.row
											.col-lg-6.col-md-6.col-sm-6
												a(href="#/node/{{b.bid}}")
												| {{row.bname}} ({{row.btype}})
											.col-lg-6.col-md-6.col-sm-6
												.fg_placeholder.forcegraph-bordered(id="fg_{{row.aid+row.bid}}" attribute-divid="fg_{{row.aid+row.bid}}" ng-force-graph attribute-readytoload=true  attribute-percent=10 attribute-nodelinks="{{row.pathlinks}}" attribute-nodes="{{row.pathnodes}}")										
		
									//td(data-title="'Relationship'" )
										| {{row.rel}}
									//td(data-title="'Name'")
										//.fg_placeholder(id="fg_{{row.aid+row.bid}}" attribute-divid="fg_{{row.aid+row.bid}}" ng-force-graph attribute-readytoload=true  attribute-percent=10 attribute-nodelinks="{{row.pathlinks}}" attribute-nodes="{{row.pathnodes}}")
										//a(href="#/node/{{b.aid}}")
											| {{row.aname}}
									//td(data-title="'Name'")
										a(href="#/node/{{b.aid}}")
											| {{row.pathnodes}}
