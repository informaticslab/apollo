// new row
.row
	.div(ng-class='getContentWidth()')
		.content_block_faq
			span.visible-md.visible-lg
				.subnav
					ul.nav.nav-pills
						li
							a(href='#/quickGuide/') Quick Guide
						li
							a(href='#/browse/') Browse
						li
							a(href='#/search/') Search
						li
							a(href='#/advancedSearch/') Relationship Explorer
						li
							a(href='' class='inactive') Favorites
						li.active
							a(href='#/inTheLab/') In the Lab
				// end subnav
			span.visible-sm
				.subnav_sm
					ul.nav.nav-pills
						li
							a(href='#/quickGuide/') Quick Guide
						li
							a(href='#/browse/') Browse
						li
							a(href='#/search/') Search
						li
							a(href='#/advancedSearch/') Relationship Explorer
						li
							a(href='' class='inactive') Favorites
						li.active
							a(href='#/inTheLab/') In the Lab
				// end subnav
			//- a.btn.btn-custom(onclick='goBack()') Back
			//- .nav_split
			//- 	.btn-group.btn-group-md.btn-group-justified
			//- 		.btn-group
			//- 			a.btn.btn-custom(href="#/dashboard", id='btnDashboard') Summary
			//- 		.btn-group
			//- 			a.btn.btn-default(href='#/dashboarddetail', id='btnDashboardDetail') Detail
			//- 	// end button group
			//- // end nav_split
			.top_block_faq
				h4.orange_main_faq
					span.glyphicon.glyphicon-comment.icon-flipped.faq_icon
					| CISP Change Request Queue
				.vertical_spacing_browse
				.row
					.col-lg-12.col-md-12.col-sm-12
						ul.nav.nav-pills
							li(role="presentation")
								a(href="#/admin")
									| Search
							li.active(role="presentation")
								a(href="#/adminCRQueue")
									| Queue
				hr
				.vertical_spacing_less
				.row
					.col-lg-12.col-md-12.col-sm-12
						.orange_header_larger
							| Change Requests Summary
				.row
					.col-lg-12.col-md-12.col-sm-12
						.list-group
							a.list-group-item(href="" ng-click="crFilterModel='UPDATE'")
								span.badge
									| {{ crUpdateCount }}
								| Update
							a.list-group-item(href="" ng-click="crFilterModel='DELETE'")
								span.badge
									| {{ crDeleteCount }}
								| Delete
							a.list-group-item(href="" ng-click="crFilterModel='ADD'")
								span.badge
									| {{ crAddCount }}
								| Add
							a.list-group-item(href="" ng-click="crFilterModel=''")
								span.badge
									| {{ crDeleteCount+crUpdateCount+crAddCount }}
								| All
				.vertical_spacing_less
				.row
					.col-lg-12.col-md-12.col-sm-12
						.orange_header_larger
							| Change Requests Records
				.row
					.col-lg-12.col-md-12.col-sm-12
						.btn-group.pull-right
							label.btn.btn-primary(ng-model="crStatusModel" btn-radio="'PENDING'" )
								| Pending
							label.btn.btn-primary(ng-model="crStatusModel" btn-radio="'APPROVED'" )
								| Approved
							label.btn.btn-primary(ng-model="crStatusModel" btn-radio="''" )
								| All
				.vertical_spacing_less
				.row.validationstatus_table_header
					.col-lg-12.col-md-12.col-sm-12
						table.table
							tr
								th.col-md-1
									| ID
								th.col-md-1
									| Node Type
								th.col-md-6
									| Name
								th.col-md-1
									| User
								th.col-md-1
									| Timestamp
								th.col-md-1
									| CR REQUEST TYPE
								th.col-md-1
									| CR STATUS
								th.col-md-1
									|   
							tr(ng-repeat="doc in mongoDocumentsAll | filter:{CR_REQUEST_TYPE:crFilterModel,CR_STATUS:crStatusModel} " ng-init="hover=false"  ng-mouseenter="hover = true" ng-mouseleave="hover = false")
								td
									| {{doc.id}}
								td
									| {{doc.CR_NODE_TYPE}}
								td
									a(href="#/adminCRQueue/CRDiff/{{doc._id}}")
										| {{doc.name}}
								td
									| {{doc.CR_USER}}
								td
									| {{ doc.CR_DATE | date : format : timezone}}
								td
									| {{doc.CR_REQUEST_TYPE}}
								td
									| {{doc.CR_STATUS}}
								td
									|   
									i.fa.fa-times(ng-show="hover")
									|    
				.vertical_spacing_lesser
				.vertical_spacing_less
						img.img-responsive.linkage_empty( id="defaultimg" src="img/ajax-loader.gif" ng-show="showVSTableLoading")
				
				//nested row 
				
				//end nested row
				
		// end content block
	// end col spec
// end row.orange_header Overview