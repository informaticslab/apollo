.row
	.div(ng-class='getContentWidth()')
		.content_block
			span.visible-md.visible-lg
				.subnav
					ul.nav.nav-pills
						li
							a(href='#/quickGuide/') Quick Guide
						li
							a(href='#/browse/') Browse
						li
							a(href='#/search/') Search
						li
							a(href='#/advancedSearch/') Relationship Explorer
						li
							a(href='' class='inactive') Favorites
						li.active
							a(href='#/inTheLab/') In the Lab
				// end subnav
			span.visible-sm
				.subnav_sm
					ul.nav.nav-pills
						li
							a(href='#/quickGuide/') Quick Guide
						li
							a(href='#/browse/') Browse
						li.active
							a(href='#/search/') Search
						li
							a(href='#/advancedSearch/') Relationship Explorer
						li
							a(href='' class='inactive') Favorites
						li.active
							a(href='#/inTheLab/') In the Lab
				// end subnav
			.detail_header 
				.detail_header
					.detail_header_text_contain_lg
						.detail_header_text
							| Advanced Visualization X: System Tree Viewer
					.share_button_linkage
						//These lines, 173 - 187, were added 
						//Single button 
						.btn-group
							button.btn.btn-default.dropdown-toggle(type='button', data-toggle='dropdown')
								span.glyphicon.glyphicon-share
								| &nbsp;Share &nbsp;&nbsp;
								span.caret
							ul.dropdown-menu(role='menu')
								li
									a(href='') Email
								li
									a(href='') Facebook
								li
									a(href='') Twitter
						//end added lines 
			// end detail header
			.top_block2
				.block_edge
					style.
						.node {
							cursor: pointer;
						}
						.node circle {
							fill: #fff;
							stroke: steelblue;
							stroke-width: 1.5px;
						}
						.node text {
							font: 10px sans-serif;
						}
						.link.treelink{
							fill: none;
							stroke: #ccc;
							stroke-width: 1.5px;
						}
					.vertical_spacing1
						div(id='tree-container')
						script(type="text/javascript", src="/apollo/app/d3/d3-sysTreeSimple.js")
				// end block_edge
	.div(ng-class='getSidebarWidth()')
		.hx_side(ng-show='!showSidebar' ng-click='toggleSidebar()')
			div(ng-Historybar)
			.button_side
				.sidebar_button
					img(src='img/tab_button_collapse.png', alt='Collapse sidebar')   
		// end hx_side
		.hx_side(ng-show='showSidebar' ng-click='toggleSidebar()')
			.blue_side_expanded
			.button_side_expanded
				.sidebar_button
					img(src='img/tab_button_expand.png', alt='Expand sidebar')   
		// end hx_side   
// row